{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/pages|pages)/_error.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/" }
  ]
} 